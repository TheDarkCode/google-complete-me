{
  "name": "google-complete-me",
  "displayName": "Google Complete Me",
  "description": "Auto-completion using Google Suggestion API, enable the suggestions whenever you need",
  "version": "1.0.2",
  "publisher": "jaydenlin",
  "engines": {
    "vscode": "^1.0.0"
  },
  "icon": "images/icon.png",
  "categories": [
    "Productivity","Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jaydenlin/google-complete-me"
  },
  "galleryBanner": {
    "color": "#1e415e",
    "theme": "dark"
  },
  "activationEvents": [  
    "onCommand:extension.googleCompleteMe",
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "keybindings": [
      {
        "command": "extension.googleCompleteMe",
        "key": "ctrl+shift+i",
        "mac": "cmd+shift+i",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^1.8.5",
    "vscode": "^0.11.0"
  },
  "dependencies": {
    "lodash": "4.13.1",
    "node-fetch": "1.5.3",
    "xml2js": "0.4.16"
  }
}
